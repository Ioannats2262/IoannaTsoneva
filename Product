package company;

public class Product {
    private String nameProduct;
    private String expiryDate;
    private String dateEntryInWarehouse;
    private String unit;
    private String availableQuantity;
    private String location;
    private String comment;

    public Product(String nameProduct, String expiryDate, String dateEntryInWarehouse, String unit,
                   String availableQuantity, String location, String comment){
        this.nameProduct=nameProduct;
        this.expiryDate=expiryDate;
        this.dateEntryInWarehouse=dateEntryInWarehouse;
        this.unit=unit;
        this.availableQuantity=availableQuantity;
        this.location=location;
        this.comment=comment;
    }

    public String getNameProduct() {
        return nameProduct;
    }

    public String getExpiryDate() {
        return expiryDate;
    }

    public String getDateEntryInWarehouse() {
        return dateEntryInWarehouse;
    }

    public String getUnit() {
        return unit;
    }

    public String getAvailableQuantity() {
        return availableQuantity;
    }

    public String getLocation() {
        return location;
    }
    public String getComment() {
        return comment;
    }
}

